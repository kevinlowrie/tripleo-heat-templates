[project]
	description = Heat templates for deploying OpenStack
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/kevin lowrie (kevinlowrie)
[access "refs/for/refs/*"]
	read = group user/kevin lowrie (kevinlowrie)
	push = group user/kevin lowrie (kevinlowrie)
	label-Code-Review = -2..+2 group user/kevin lowrie (kevinlowrie)
	label-Verified = -1..+1 group user/kevin lowrie (kevinlowrie)
	rebase = group user/kevin lowrie (kevinlowrie)
	removeReviewer = group user/kevin lowrie (kevinlowrie)
	submit = group user/kevin lowrie (kevinlowrie)
[access "refs/heads/*"]
	read = group user/kevin lowrie (kevinlowrie)
	create = group user/kevin lowrie (kevinlowrie)
	push = +force group user/kevin lowrie (kevinlowrie)
	pushMerge = group user/kevin lowrie (kevinlowrie)
[access "refs/tags/*"]
	push = +force group user/kevin lowrie (kevinlowrie)
	pushTag = group user/kevin lowrie (kevinlowrie)
	pushSignedTag = group user/kevin lowrie (kevinlowrie)
